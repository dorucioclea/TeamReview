@using TeamReview.Web.ViewModels
@model ReportViewModel

<h3>@Model.Title</h3>
<div class="block-top">
    <p class="info">@Model.Description</p>
    <div id="@Model.FieldId"
         class="jqplot-report"
         data-is-stack-ranking="true"
         data-data-rows='@(Html.Raw(Model.DataRows))'
         data-data-rows-labels='@(Html.Raw(Model.DataRowsLabels))'
         data-xaxis-labels='@(Html.Raw(Model.XAxisLabels))'
         data-category-count='@(Model.CategoryCount)'></div>
</div>